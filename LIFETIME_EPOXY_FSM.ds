													date_of_day = li_val.Date_field.getDayOfWeek();
													info date_of_day;
													if(date_of_day == 1)
													{
														info "sunday";
														temp_hours = li_val.Hours1;
														or_time = li_val.Hours1.toList(":");
														hrs = or_time.get(0).toLong();
														mins = or_time.get(1).toLong();
